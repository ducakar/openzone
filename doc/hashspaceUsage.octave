#!/usr/bin/octave -q

VECTOR_AVG_CAPACITY = 7;
PTR_SIZE = 4;

sectorSize = ( 1 + 1 ) * PTR_SIZE + 6 * 2 + 4;

if length( argv ) ~= 2
  disp( 'usage: hashspaceUsage sectorDim sizeInMetres' );
  return;
end

dim = str2double( argv()(1) );
size = str2double( argv()(2) );

% get
nFields = floor( ( size - 1 ) / dim ) + 1;
nFields = nFields * nFields;

% size
size = nFields * sectorSize / 1024 / 1024;

printf( '%.3f MiB', size );
